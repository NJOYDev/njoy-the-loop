{"components/common/quick-search":"{{{ snippet \"forms_search\"}}}\n<form class=\"form search_wrap\"  action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\n            <div class=\"form-prefixPostfix\">\n               <input class=\"form-input\" id=\"search_query_desktop\" name=\"search_query_adv\" value=\"{{forms.search.query}}\" placeholder=\"search NJOY\">\n                <button class=\"search\"><span class=\"icon icon-search-icon\"><svg><use xlink:href=\"#icon-search\" /></svg></span></button>\n            </div>\n        </div>\n    </fieldset>\n</form>\n"}
